{
  "settings" : {
      "number_of_shards" : 1
  },
  "mappings" : {
    "_doc" : {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "epoch_second"
        },
        "humidity": {
          "type": "float"
        },
        "temp": {
          "type": "float"
        },
        "light": {
          "type": "integer"
        },
        "time": {
          "type": "date",
          "format":"YYYY-MM-dd HH:mm:ss.SSSSSS"
        }
      }
    }
  }
}